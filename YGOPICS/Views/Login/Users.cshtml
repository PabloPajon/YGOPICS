@model List<User>

<h1>Usuarios:</h1>
    <table>
        <thead>
            <tr>
                <th>Nombre</th>
            </tr>
        </thead>
        <tbody>
        @foreach(var user in Model)
        {
            <tr>
                <td>@Html.DisplayFor(m=>user.Name)</td>
            </tr>
        }
        </tbody>
</table>